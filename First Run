#include <iostream>
using namespace std;

int main() {
  cout << "\nC++ Practice What I Learn Here Display Output There" << endl;
  int a,b;
  a= 3;
  b= 5;
  bool result;

  cout << "\n";
  cout << "Value a=  " << a << endl;
  cout << "Value b=  " << b << endl;
  cout << "\n";
  //Operations +,-,*,/,%
  cout << "  //Operations +,-,*,/,%" << endl;
  cout << "a+b= " << (a+b) << endl;
  cout << "a-b= " << (a - b) << endl;
  cout << "a/b= " << (a / b) << endl;
  cout << "a*b= " << (a * b) << endl;
  cout << "a%b= " << (a % b) << endl;
  cout << "\n";
  //asighning Operatiors to Operations =
  cout << "  //asighning Operatiors to Operations =" << endl;
  cout << "(a +=b) (a=a+b)" << endl;
  cout << "(a -=b) (a=a-b)" << endl;
  cout << "(a /=b) (a=a/b)" << endl;
  cout << "(a *=b) (a=a*b)" << endl;
  cout << "(a %=b) (a=a%b)" << endl;
  cout << "\n";
  cout << "a= " << a << endl;
  cout << "b= " << b << endl;
  cout << "\nafter a+=b; = 8" << endl;
  cout << "\n";
  //relational Operators
  cout << "  //relational Operators ==,!=,>,<,>=,<=" << endl;
  result = (a == b);
  cout << "3 == 5 is " << result << endl;
  result = (a != b);
  cout << "3 != 5 is " << result << endl;
  result = (a > b);
  cout << "3 > 5 is " << result << endl;
  result = (a < b);
  cout << "3 < 5 is " << result << endl;
  result = (a >= b);
  cout << "3 >= 5 is " << result << endl;
  result = (a <= b);
  cout << "3 <= 5 is " << result << endl;

        return 0;
}
